<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender"
			autocreate="true" generate="true" type="list" />
		<collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum"
			autocreate="true" generate="true" type="set" />
	</collectiontypes>

	<enumtypes>
		<enumtype generate="true" code="SwatchColorEnum" autocreate="true"
			dynamic="true">
			<value code="BLACK" />
			<value code="BLUE" />
			<value code="BROWN" />
			<value code="GREEN" />
			<value code="GREY" />
			<value code="ORANGE" />
			<value code="PINK" />
			<value code="PURPLE" />
			<value code="RED" />
			<value code="SILVER" />
			<value code="WHITE" />
			<value code="YELLOW" />
		</enumtype>

		<enumtype generate="true" code="giorniSettEnum" autocreate="true"
			dynamic="true">
			<value code="lunedi" />
			<value code="martedi" />
			<value code="mercoledi" />
			<value code="giovedi" />
			<value code="venerdi" />
			<value code="sabato" />
			<value code="domenica" />
		</enumtype>
	</enumtypes>

	<relations>
		<relation code="Volo2Aereo" generate="true" localized="false" autocreate="true">
	            <sourceElement type="Volo" cardinality="one" qualifier="tipoAereo"></sourceElement>
	            <targetElement type="Aereo" cardinality="many" qualifier="tipoAereo"></targetElement>
	    </relation>
	    <relation code="Volo2AeroportoPartRelation" generate="true" localized="false" autocreate="true">
	            <sourceElement type="Volo" cardinality="one" qualifier="aeroportoPart"></sourceElement>
	            <targetElement type="Aeroporto" cardinality="many" qualifier="id"></targetElement>
	    </relation>
	    <relation code="Volo2AeroportoArrRelation" generate="true" localized="false" autocreate="true">
	            <sourceElement type="Volo" cardinality="one" qualifier="aeroportoArr"></sourceElement>
	            <targetElement type="Aeroporto" cardinality="many" qualifier="id"></targetElement>
	    </relation>
	</relations>

	<itemtypes>

		<!-- Add your item definitions here -->

		<itemtype code="Aeroporto" generate="true" autocreate="true">
			<deployment table="aeroporto" typecode="12345" />
			<attributes>
				<attribute qualifier="id" type="java.lang.Integer">
					<description>Id aeroporto</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="citta" type="java.lang.String">
					<description>citta aeroporto</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="nazione" type="java.lang.String">
					<description>nazione aeroporto</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="npiste" type="java.lang.Integer">
					<description>numero piste aeroporto</description>
					<persistence type="property"></persistence>
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Aereo" generate="true" autocreate="true">
			<deployment table="aero" typecode="12346" />
			<attributes>
				<attribute qualifier="tipoAereo" type="java.lang.String">
					<description>tipo di aereo</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="nPass" type="java.lang.Integer">
					<description>numero di passeggeri</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="quantitaMerci" type="java.lang.Integer">
					<description>Numero delle merci trasportabili</description>
					<persistence type="property"></persistence>
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Volo" generate="true" autocreate="true">
			<deployment table="volo" typecode="12347" />
			<attributes>
				<attribute qualifier="idVolo" type="java.lang.Integer">
					<description>id volo</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="giornoSett" type="giornoSettEnum">
					<description>giorno della settimana </description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="aeroportoPart" type="java.lang.Integer">
					<description>id dell'aeroporto di partenza</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="aeroportoArr" type="java.lang.Integer">
					<description>id dell'aeroporto di arrivo</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="tipoAereo" type="java.lang.String">
					<description>nome dell'aereo</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="oraPartenza" type="java.lang.integer">
					<description>ora partenza del volo</description>
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="oraArrivo" type="java.lang.integer">
					<description>ora arrivo del volo</description>
					<persistence type="property"></persistence>
				</attribute>
			</attributes>
		</itemtype>

		<!-- TypeGroups are for structure of this file only -->

		<typegroup name="Apparel">
			<itemtype code="ApparelProduct" extends="Product"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelProduct">
				<description>Base apparel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the ApparelProduct is designed for</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>

			<itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelStyleVariantProduct">
				<description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
				<attributes>
					<attribute qualifier="style" type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelSizeVariantProduct">
				<description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
				<attributes>
					<attribute qualifier="size" type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Electronics">
			<itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ElectronicsColorVariantProduct">
				<description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
				<attributes>
					<attribute qualifier="color" type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

	</itemtypes>
</items>
